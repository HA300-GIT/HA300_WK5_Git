COLUMN TABLE "Util.SSCOOKIE" (
     "SESSIONID" INTEGER  GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000000000 INCREMENT BY 1 MAXVALUE 1999999999) COMMENT 'Session ID',
	 "NAME" NVARCHAR(30) COMMENT 'Cookie Name',
	 "APPLICATION" NVARCHAR(128) COMMENT 'Application',
	 "EXPIRY" LONGDATE  COMMENT 'Expiration Timestamp',
	 "DATA" NCLOB MEMORY THRESHOLD 1000 COMMENT 'Cookie Data',
	 PRIMARY KEY ("SESSIONID", "NAME", "APPLICATION")) 
	 COMMENT 'Server Side Cookies'
	 UNLOAD PRIORITY 5 AUTO MERGE 
