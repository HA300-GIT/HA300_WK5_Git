COLUMN TABLE "EPM_CUSTOMER_ORDERS" (
	"ORDER_ID" NVARCHAR(10)  NOT NULL COMMENT 'Order ID',
     "CUSTOMER_ID" NVARCHAR(10)  COMMENT 'Customer ID',
     "PRODUCT_ID" NVARCHAR(10)  COMMENT 'Product ID',
	 "ORDER_DATE" DATE COMMENT 'Order Date',
	 "ORDER_QTY" INTEGER COMMENT 'Order Quantity',
	 "AMOUNT" DECIMAL(10,2) COMMENT 'Order Amount',
	 PRIMARY KEY ("ORDER_ID")) 
	 COMMENT 'Customer Order Details'
	 