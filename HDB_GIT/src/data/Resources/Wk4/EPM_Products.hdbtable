COLUMN TABLE "EPM_PRODUCTS" (
     "PRODUCT_ID" NVARCHAR(10) NOT NULL COMMENT 'Product ID',
	 "PRODUCT_NAME" NVARCHAR(20) COMMENT 'Product Name',
	 "PRODUCT_CAT" NVARCHAR(15)  COMMENT 'Product Category',
	 "PRODUCT_SUBCAT" NVARCHAR(15) COMMENT 'Product Subcategory',
	 PRIMARY KEY ("PRODUCT_ID")) 
	 COMMENT 'EPM Products table'
	 UNLOAD PRIORITY 5 AUTO MERGE 